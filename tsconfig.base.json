{
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es2016"
    ],
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
