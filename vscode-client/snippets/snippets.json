{
    "shebang": {
        "description": "[sh]hebang",
        "prefix": "sh",
        "body": [
            "#!/usr/bin/env bash"
        ]
    },
    "echo": {
        "description": "[ec]ho",
        "prefix": "ec",
        "body": [
            "echo \"${0:message}\""
        ]
    },
    "printf": {
        "description": "[pr]intf",
        "prefix": "pr",
        "body": [
            "printf '%s' \"${0:message}\""
        ]
    },
    "if": {
        "description": "[if]",
        "prefix": "if",
        "body": [
            "if ${0:command}; then",
