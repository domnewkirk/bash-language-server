{
    "shebang": {
        "description": "[sh]hebang",
        "prefix": "sh",
        "body": [
            "#!/usr/bin/env ${1|bash,sh|}"
        ]
    },
    "if": {
        "description": "[if]",
        "prefix": "if",
        "body": [
            "if ${1:command}; then",
            "\t$0",
            "fi"
        ]
    },
    "ife": {
        "description": "[if] [e]lse",
        "prefix": "ife",
        "body": [
            "if ${1:command}; then",
            "\techo",
            "else",
            "\t$0",
            "fi"
        ]
    },
    "wh": {
        "description": "[wh]ile",
        "prefix": "wh",
        "body": [
            "while ${1:command}; do",
            "\t$0",
            "done"
        ]
    },
    "un": {
        "description": "[un]til",
        "prefix": "un",
        "body": [
            "until ${1:command}; do",
            "\t$0",
            "done"
        ]
    },
    "fo": {
        "description": "[fo]r",
        "prefix": "fo",
        "body": [
            "for ${1:variable} in ${2:list}; do",
            "\t$0",
            "done"
        ]
    },
    "fu": {
        "description": "[fu]nction",
        "prefix": "fu",
        "body": [
            "${1:function_name}() {",
            "\t$0",
            "}"
        ]
    },
    "ma": {
        "description": "[ma]in",
        "prefix": "ma",
        "body": [
            "main() {",
            "\t$0",
            "}"
        ]
    },
    ":-": {
        "description": "[:-] expansion",
        "prefix": ":-",
        "body": [
            "\"\\${${1:variable}:-${2:default}}\""
        ]
    },
    ":=": {
        "description": "[:=] expansion",
        "prefix": ":=",
        "body": [
            "\"\\${${1:variable}:=${2:default}}\""
        ]
    },
    ":?": {
        "description": "[:?] expansion",
        "prefix": ":?",
        "body": [
            "\"\\${${1:variable}:?${2:error_message}}\""
        ]
    },
    ":+": {
        "description": "[:+] expansion",
        "prefix": ":+",
        "body": [
            "\"\\${${1:variable}:+${2:alternative}}\""
        ]
    },
    "#": {
        "description": "[#] expansion",
        "prefix": "#",
        "body": [
            "\"\\${${1:variable}#${2:pattern}}\""
        ]
    },
    "##": {
        "description": "[##] expansion",
        "prefix": "##",
        "body": [
            "\"\\${${1:variable}##${2:pattern}}\""
        ]
    },
    "%": {
        "description": "[%] expansion",
        "prefix": "%",
        "body": [
            "\"\\${${1:variable}%${2:pattern}}\""
        ]
    },
    "%%": {
        "description": "[%%] expansion",
        "prefix": "%%",
        "body": [
            "\"\\${${1:variable}%%${2:pattern}}\""
        ]
    },
    "..": {
        "description": "brace expansion",
        "prefix": "..",
        "body": [
            "{${1:from}..${2:to}}"
        ]
    },
    "echo": {
        "description": "[echo]",
        "prefix": "echo",
        "body": [
            "echo \"${1:message}\""
        ]
    },
    "printf": {
        "description": "[printf]",
        "prefix": "printf",
        "body": [
            "printf '%s' \"${1:message}\""
        ]
    },
    "source": {
        "description": "[source]",
        "prefix": "source",
        "body": [
            "source \"${1:path/to/file}\""
        ]
    },
    "alias": {
        "description": "[alias]",
        "prefix": "alias",
        "body": [
            "alias ${1:name}=${2:value}"
        ]
    },
    "builtin": {
        "description": "[builtin]",
        "prefix": "builtin",
        "body": [
            "builtin ${1|source,alias,bg,bind,break,builtin,caller,cd,command,compgen,complete,complete,compopt,continue,declare,typeset,dirs,disown,echo,enable,eval,exec,exit,export,fc,fg,getopts,hash,help,history,jobs,kill,let,local,logout,mapfile,readarray,popd,pushd,pwd,read,readonly,return,set,shift,shopt,suspend,test,type,ulimit,umask,unalias,unset|}"
        ]
    },
    "cd": {
        "description": "[cd]",
        "prefix": "cd",
        "body": [
            "cd \"${1:path/to/directory}\" || echo \"${2:error_message}\" >&2"
        ]
    },
    "getopts": {
        "description": "[getopts]",
        "prefix": "getopts",
        "body": [
            "getopts ${1:optstring} ${2:name}"
        ]
    },
    "jobs": {
        "description": "[jobs]",
        "prefix": "jobs",
        "body": [
            "jobs -x ${1:command}"
        ]
    },
    "kill": {
        "description": "[kill]",
        "prefix": "kill",
        "body": [
            "kill ${1|-l,-L|}"
        ]
    },
    "let": {
        "description": "[let]",
        "prefix": "let",
        "body": [
            "let ${1:argument}"
        ]
    },
    "test": {
        "description": "[test]",
        "prefix": "test",
        "body": [
            "[[ ${1:argument1} ${2|-ef,-nt,-ot,==,=,!=,<,>,-eq,-ne,-lt,-le,-gt,-ge|} ${3:argument2} ]]"
        ]
    },
    "de": {
        "description": "[de]vice name",
        "prefix": "de",
        "body": [
            "/dev/${1|null,stdin,stdout,stderr|}"
        ]
    }
}