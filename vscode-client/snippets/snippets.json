            "\t$0",
            "fi"
        ]
    },
    "wh": {
        "description": "[wh]ile",
        "prefix": "wh",
        "body": [
            "while ${0:command}; do",
            "\t$0",
            "done"
        ]
    },
    "un": {
        "description": "[un]til",
        "prefix": "un",
        "body": [
            "until ${0:command}; do",
            "\t$0",
            "done"
        ]
    },
    "fo": {
        "description": "[fo]r",
        "prefix": "fo",
